  "scripts": {
    "postinstall": "echo --Run postinstall tasks-- && npm-run-all build-dist-prod build-dist-dev",
    "stylelint": "echo --Styles linting-- && stylelint src/**/*.{css,pcss} --syntax scss && echo No styles problems found",
    "eslint": "echo --JS source code linting-- && eslint src --ext .js && echo No code problems found",
    "lint": "echo --Lint all sources-- && npm-run-all stylelint eslint",
    "jestClearCache": "echo --Clear jest cache-- && jest --clearCache",
    "jest": "echo --Run tests-- && jest --verbose",
    "test": "echo --Run all lintings and tests-- && npm-run-all lint jest",
    "tags": "ctags -R --exclude=@.ctagsignore .",
    "cosmos": "cosmos --mode development",
    "cosmos-export": "cosmos-export",
    "webpack-stats": "echo --Generate webpack stats-- && webpack --env.DIST --profile --json > .webpack-stats.json",
    "webpack-stats-analyze": "echo --Analyze webpack stats-- && npm run -s webpack-stats && webpack-bundle-analyzer .webpack-stats.json build",
    "webpack": "webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --hot --inline --watch && flow status",
    "server": "npm run -s start",
    "build-dist-prod": "echo --Build production-- && webpack --env.DIST --mode production",
    "build-dist-dev": "echo --Build development-- && webpack --env.DIST --mode development",
    "build-dist-dev-watch": "echo --Build development and watch sources-- && webpack --env.DIST --mode development --watch",
    "build-demo-prod": "echo --Build production-- && webpack --mode production",
    "build-demo-dev": "echo --Build development-- && webpack --mode development",
    "build-demo-dev-watch": "echo --Build development and watch sources-- && webpack --mode development --watch",
    "build-prod": "npm run -s build-dist-prod",
    "build-dev": "npm run -s build-dist-dev",
    "build-dev-watch": "npm run -s build-dist-dev-watch",
    "build": "npm run -s build-dist-prod",
    "build-all": "npm-run-all build-dist-dev build-dev-prod build-demo-dev build-demo-prod",
    "update-changed-tag": "echo --Update changed tag file with current timestamp-- && node util-update-changed.js",
    "help": "echo --List all available script commands-- && npm-package-user-scripts-list"
  }
