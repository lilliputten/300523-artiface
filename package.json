  "scripts": {
    "postinstall-patches-UNUSED": "echo --Apply patches (posix commands used)-- && cd \"!Patches\" && pwd && sh ./patch-tool-apply-all && cd ..",
    "postinstall-publish-submodule": "echo --Install publish submodule (posix commands used)-- && sh ./util-publish-init.sh",
    "postinstall": "echo --Run postinstall tasks-- && npm-run-all postinstall-publish-submodule",
    "stylelint": "echo --Styles linting-- && stylelint src/**/*.{css,pcss} --syntax scss && echo No styles problems found",
    "jslint": "echo --JS source code linting-- && eslint --max-warnings=0 src --ext js,jsx && echo No JS code problems found",
    "tslint": "echo --TS source code linting-- && tslint -c tslint.yaml --project tsconfig.json \"src/*\" && echo No TS code problems found",
    "lint": "echo --Lint all sources-- && npm-run-all stylelint tslint jslint",
    "jest-clear-cache": "echo --Clear jest cache-- && jest --clearCache && echo Ok",
    "webpack-clear-cache": "echo --Clear webpack cache-- && rm -rf node_modules/.cache && echo Ok",
    "pretest": "npm run lint",
    "prebuild-UNUSED": "npm run clean",
    "jest": "echo --Run tests-- && jest --verbose",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow",
    "tags": "ctags -R --exclude=@.ctagsignore .",
    "cosmos-classic": "cosmos-classic --mode development",
    "cosmos-classic-export": "cosmos-classic-export",
    "cosmos": "cosmos --mode development",
    "cosmos-export": "cosmos-export",
    "publish": "echo --Publish build (posix commands used)-- && sh ./util-publish.sh",
    "webpack-stats-dev": "echo --Generate webpack stats-- && webpack --profile --json --env.DIST --mode development > .webpack-stats.json",
    "webpack-stats-analyze-dev": "echo --Analyze webpack stats-- && npm run -s webpack-stats && webpack-bundle-analyzer .webpack-stats.json build-dev",
    "webpack-stats": "echo --Generate webpack stats-- && webpack --profile --json --env.DIST > .webpack-stats.json",
    "webpack-stats-analyze": "echo --Analyze webpack stats-- && npm run -s webpack-stats && webpack-bundle-analyzer .webpack-stats.json build",
    "server": "echo --Run demo server-- && webpack serve --mode development --hot --inline",
    "start": "echo --Run demo server (alias)-- && npm run -s server --",
    "server-OLD": "echo --Run demo server-- && webpack-dev-server --mode development --hot --inline --watch",
    "build-prod": "echo --Build production library-- && webpack --env DIST --mode production",
    "build-dev": "echo --Build development library-- && webpack --env DIST --mode development",
    "build-dev-watch": "echo --Build development library and watch changes-- && webpack --env DIST --mode development --watch",
    "build-demo-prod": "echo --Build production demo-- && webpack --mode production",
    "build-demo-dev": "echo --Build development demo-- && webpack --mode development",
    "build-demo-dev-watch": "echo --Build development demo and watch changes-- && webpack --mode development --watch",
    "webpack": "echo --Build development demo and watch changes (alias to build-demo-dev-watch)-- && webpack --mode development --watch",
    "build": "npm run -s build-dev",
    "build-all": "npm-run-all build-dev build-prod build-demo-dev build-demo-prod",
    "increment-version": "echo --Increment patch version-- && node util-increment-version.js",
    "update-build-time": "echo --Update build timestamp-- && node util-update-build-time.js",
    "update-build-variables": "echo --Update build properties-- && sh util-update-build-variables.sh",
    "help": "echo --List all available script commands-- && npm-package-user-scripts-list"
